<tau:UserControl x:Class="Tauron.Application.Deployment.AutoUpload.Views.VersionRepoManagerCommand.VersionRepoSelectView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:tau="http://schemas.TauronWpf.com"
             xmlns:local="clr-namespace:Tauron.Application.Deployment.AutoUpload.ViewModels.VersionRepoManagerCommand"
             xmlns:viws="clr-namespace:Tauron.Application.Deployment.AutoUpload.Views.Common"
             mc:Ignorable="d" d:DataContext="{d:DesignInstance local:VersionRepoSelectViewModel}"
             d:DesignHeight="450" d:DesignWidth="800" Background="White">
    <viws:PageTemplate Title="Version Repository">
        <ContentControl Content="{Binding RepoSelector}"/>
    </viws:PageTemplate>
</tau:UserControl>
