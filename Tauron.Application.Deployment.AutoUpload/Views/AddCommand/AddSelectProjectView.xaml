<tau:UserControl x:Class="Tauron.Application.Deployment.AutoUpload.Views.AddCommand.AddSelectProjectView"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:local="clr-namespace:Tauron.Application.Deployment.AutoUpload.ViewModels.AddCommand"
                 xmlns:com="clr-namespace:Tauron.Application.Deployment.AutoUpload.Views.Common"
                 xmlns:tau="http://schemas.TauronWpf.com"
                 mc:Ignorable="d" Background="White" d:DataContext="{d:DesignInstance local:AddSelectProjectViewModel}"
                 d:DesignHeight="450" d:DesignWidth="800">
    <com:PageTemplate Title="Projekt Wahl">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <TextBlock Grid.Row="0" Grid.Column="0" Text="Projekt Name:" Margin="5" />
            <ContentControl Grid.Row="0" Grid.Column="1" Content="{Binding ProjectSelector}" Margin="5" />
        </Grid>
    </com:PageTemplate>
</tau:UserControl>