    <DataTemplate x:Key="HeaderDataTemplate" DataType="{x:Type syncfusion:TabItemExt}">
        <TextBlock Focusable="False" Text="{Binding FallbackValue=''}"  TextTrimming="CharacterEllipsis" IsHitTestVisible="False" Padding="1,0,0,1" />
