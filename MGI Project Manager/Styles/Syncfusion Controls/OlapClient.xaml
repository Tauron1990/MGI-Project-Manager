    <DataTemplate x:Key="EditableDataTemplate" DataType="{x:Type syncfusion:TabItemExt}">
        <TextBox x:Name="EditableHeader" Focusable="True" Style="{Binding Path=EditHeaderItemStyle, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type syncfusion:TabControlExt}}}"
				 Text="{Binding Path=Header, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type syncfusion:TabItemExt}}, Mode=TwoWay}"  IsReadOnly="False" Padding="4,1,4,1" />
    </DataTemplate>
    <ControlTemplate x:Key="Office2010BlackCloseButtonTemplate" TargetType="{x:Type ButtonBase}">
        <shared:Border3D x:Name="brdBack" BorderThickness="1" BorderBrush="{StaticResource Office2010BlackCloseButtonBorderBrush}">
            <Border x:Name="InnerBorder" BorderThickness="1" BorderBrush="{StaticResource CloseButtonBorderBrush}">
                <Path Name="pathButton" SnapsToDevicePixels="True"
				Stroke="{StaticResource Office2010BlackCloseButtonPathBrush}"
				StrokeThickness="1.5"
                Data="M1,1 L7,7 M1,7 L7,1"
				HorizontalAlignment="Center" VerticalAlignment="Center"
				Width="8" Height="8"/>
            </Border>
        </shared:Border3D>
        <ControlTemplate.Triggers>
            <Trigger Property="shared:SkinStorage.EnableTouch" Value="true">
                <Setter Property="Height" Value="13" TargetName="pathButton"/>
                <Setter Property="Width" Value="13" TargetName="pathButton"/>
                <Setter Property="Data" Value="M1,1 L12,12 M1,12 L12,1" TargetName="pathButton"/>
                <Setter Property="StrokeThickness" Value="2.5" TargetName="pathButton"/>



            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="InnerBorder" Property="Background" Value="{StaticResource ButtonAdvPressedBackgroundBrush}"/>
                <Setter TargetName="brdBack" Property="BorderBrush" Value="{StaticResource ButtonAdvPressedBorderBrush}"/>
                <Setter TargetName="InnerBorder" Property="BorderBrush" Value="{StaticResource ButtonAdvPressedInnerBorderBrush}"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition  Property="IsMouseOver" Value="True"/>
                    <Condition Property="shared:SkinStorage.EnableTouch" Value="true"/>
                </MultiTrigger.Conditions>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition  Property="IsMouseOver" Value="True"/>
                    <Condition Property="shared:SkinStorage.EnableTouch" Value="false"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="InnerBorder" Property="Background" Value="{StaticResource CloseButtonHoverBackground}"/>
                <Setter TargetName="brdBack" Property="BorderBrush" Value="{StaticResource CloseButtonHoverBorderBrush}"/>
                <Setter TargetName="InnerBorder" Property="BorderBrush" Value="#FFFFFFFF"/>
            </MultiTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
