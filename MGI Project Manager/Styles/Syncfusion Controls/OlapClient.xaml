                            <Setter Property="LayoutTransform" TargetName="PART_DockPanel">
                                <Setter.Value>
                                    <RotateTransform Angle="-180"/>
                                </Setter.Value>
                            </Setter>
                        </DataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=TabStripPlacement,RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type syncfusion:TabControlExt}},FallbackValue=Top}" Value="Bottom"/>
                                <Condition Binding="{Binding Path=(syncfusion:TabControlExt.ImageAlignment), RelativeSource={RelativeSource Self}}" Value="LeftOfText"/>
                            </MultiDataTrigger.Conditions>
                            <Setter Property="Margin" Value="8,0,0,0" TargetName="PART_Image"/>
