    <Style x:Key="CheckBoxListBoxItemStyle" TargetType="ListBoxItem">
        <Setter Property="Foreground" Value="#FF000000" />
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="IsSelected" Value="{Binding Path=IsSelected}"></Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ListBoxItem">
                    <Grid>
                        <CheckBox  Content="{Binding Path=DisplayField}" IsChecked="{Binding Path=IsSelected, Mode=TwoWay}" ClickMode="Press"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <ControlTemplate x:Key="ToggleButtonStyle" TargetType="ToggleButton">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="25"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition Width="20"/>
            </Grid.ColumnDefinitions>
            <Border x:Name="ButtonBorder" Background="{TemplateBinding Background}" Grid.ColumnSpan="2" BorderBrush=" # FF707070" SnapsToDevicePixels="True" BorderThickness="1">                
            </Border>
            <TextBlock Margin="5,0,0,0" VerticalAlignment="Center" Text="{TemplateBinding Content}"  ToolTip="{TemplateBinding Content}"></TextBlock>
            <Path x:Name="ToggleButtonArrow" Grid.Column="1" HorizontalAlignment="Center" Margin="3,0,0,0" VerticalAlignment="Center" Fill="Black" Data="{StaticResource DownArrowGeometry}"/>
        </Grid>

        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Fill" TargetName="ToggleButtonArrow" Value="#AFAFAF"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="ButtonBorder" Property="BorderBrush" Value="#99C1E2"/>
            </Trigger>
            <Trigger Property="IsChecked" Value="True">
                <Setter TargetName="ButtonBorder" Property="BorderBrush" Value="#C0A776"/>
            </Trigger>            
        </ControlTemplate.Triggers>
    </ControlTemplate>

