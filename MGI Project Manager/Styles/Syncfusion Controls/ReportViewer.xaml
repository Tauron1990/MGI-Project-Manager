	<Style x:Key="MetroButtonStyle"  TargetType="Button">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border x:Name="MyBackgroundElement" BorderThickness="0" Background="Transparent">
                        <ContentPresenter x:Name="ButtonContentPresenter" VerticalAlignment="Center" HorizontalAlignment="Center"/>                        
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="MyBackgroundElement" Property="Border.BorderBrush" Value="{StaticResource HoverBorderBrush}"/>
                            <Setter TargetName="MyBackgroundElement" Property="Background" Value="{StaticResource HoverBackgroundBrush}"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="False">
                            <Setter TargetName="MyBackgroundElement" Property="Border.BorderBrush" Value="{StaticResource BorderBrush}"/>
                            <Setter TargetName="MyBackgroundElement" Property="Background" Value="{StaticResource BackgroundBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="MyBackgroundElement" Property="Border.BorderBrush" Value="{StaticResource ActiveBorderBrush}"/>
                            <Setter TargetName="MyBackgroundElement" Property="Background" Value="{StaticResource ActiveBackgroundBrush}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="ButtonContentPresenter" Property="Opacity" Value="0.5"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>	

    <Geometry x:Key="MetroDownArrowGeometry">M 0 0 L 3.5 4 L 7 0 Z</Geometry>

    <ControlTemplate x:Key="MetroToggleButtonStyle" TargetType="ToggleButton">        
        <Grid FocusVisualStyle="{x:Null}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition Width="20"/>
            </Grid.ColumnDefinitions>
            <Border x:Name="ButtonBorder" FocusVisualStyle="{x:Null}" Background="{TemplateBinding Background}" BorderBrush="Transparent" Grid.ColumnSpan="2" CornerRadius="0" BorderThickness="1" />
            <Path x:Name="ToggleButtonArrow" Grid.Column="1" HorizontalAlignment="Center" Margin="3,1,0,0" VerticalAlignment="Center" Fill="#FF333333" Data="{StaticResource MetroDownArrowGeometry}"/>
        </Grid>

        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition  Property="ToggleButton.IsMouseOver" Value="True"/>
                    <Condition Property="IsPressed" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="ButtonBorder" Property="Border.BorderBrush" Value="{StaticResource HoverBorderBrush}"/>
                <Setter TargetName="ButtonBorder" Property="Background" Value="{StaticResource HoverBackgroundBrush}"/>
                <Setter TargetName="ToggleButtonArrow" Property="Fill" Value="{StaticResource  HoverGlyphBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition  Property="ToggleButton.IsMouseOver" Value="True"/>
                    <Condition Property="IsPressed" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="ButtonBorder" Property="Border.BorderBrush" Value="{StaticResource ActiveBorderBrush}"/>
                <Setter TargetName="ButtonBorder" Property="Background" Value="{StaticResource ActiveBackgroundBrush}"/>
                <Setter TargetName="ToggleButtonArrow" Property="Fill" Value="{StaticResource ActiveGlyphBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition  Property="IsChecked" Value="True"/>
                    <Condition Property="IsMouseOver" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="ButtonBorder" Property="Border.BorderBrush" Value="{StaticResource ActiveBorderBrush}"/>
                <Setter TargetName="ButtonBorder" Property="Background" Value="{StaticResource ActiveBackgroundBrush}"/>
                <Setter TargetName="ToggleButtonArrow" Property="Fill" Value="{StaticResource ActiveGlyphBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition  Property="IsChecked" Value="False"/>
                    <Condition Property="IsMouseOver" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="ButtonBorder" Property="Border.BorderBrush" Value="{StaticResource BorderBrush}"/>
                <Setter TargetName="ButtonBorder" Property="Background" Value="{StaticResource BackgroundBrush}"/>
                <Setter TargetName="ToggleButtonArrow" Property="Fill" Value="{StaticResource GlyphBrush}"/>
            </MultiTrigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="ToggleButtonArrow" Property="Opacity" Value="0.5"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
	
	<ControlTemplate x:Key="MetroZoomToggleButtonStyle" TargetType="ToggleButton">        
        <Grid FocusVisualStyle="{x:Null}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition Width="20"/>
            </Grid.ColumnDefinitions>
            <Border x:Name="ButtonBorder" FocusVisualStyle="{x:Null}" Background="{TemplateBinding Background}" BorderBrush="Transparent" Grid.ColumnSpan="2" CornerRadius="0" BorderThickness="1" >
                <Border x:Name="splitBorder" FocusVisualStyle="{x:Null}" BorderBrush="{StaticResource BorderBrush}" BorderThickness="1" HorizontalAlignment="Right" SnapsToDevicePixels="true" Width="{DynamicResource {x:Static SystemParameters.VerticalScrollBarWidthKey}}">
                    <Path x:Name="ToggleButtonArrow" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Fill="#FF333333" Data="{StaticResource MetroDownArrowGeometry}"/>
			</Border>
            </Border>
        </Grid>

        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition  Property="ToggleButton.IsMouseOver" Value="True"/>
                    <Condition Property="IsPressed" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="splitBorder" Property="Border.BorderBrush" Value="{StaticResource HoverBorderBrush}"/>
                <Setter TargetName="splitBorder" Property="Background" Value="{StaticResource HoverBackgroundBrush}"/>
                <Setter TargetName="ToggleButtonArrow" Property="Fill" Value="{StaticResource  HoverGlyphBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition  Property="ToggleButton.IsMouseOver" Value="True"/>
                    <Condition Property="IsPressed" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="splitBorder" Property="Border.BorderBrush" Value="{StaticResource ActiveBorderBrush}"/>
                <Setter TargetName="splitBorder" Property="Background" Value="{StaticResource ActiveBackgroundBrush}"/>
                <Setter TargetName="ToggleButtonArrow" Property="Fill" Value="{StaticResource ActiveGlyphBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition  Property="IsChecked" Value="True"/>
                    <Condition Property="IsMouseOver" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="splitBorder" Property="Border.BorderBrush" Value="{StaticResource ActiveBorderBrush}"/>
                <Setter TargetName="splitBorder" Property="Background" Value="{StaticResource ActiveBackgroundBrush}"/>
                <Setter TargetName="ToggleButtonArrow" Property="Fill" Value="{StaticResource ActiveGlyphBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition  Property="IsChecked" Value="False"/>
                    <Condition Property="IsMouseOver" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="splitBorder" Property="Border.BorderBrush" Value="{StaticResource BorderBrush}"/>
                <Setter TargetName="splitBorder" Property="Background" Value="{StaticResource BackgroundBrush}"/>
                <Setter TargetName="ToggleButtonArrow" Property="Fill" Value="{StaticResource GlyphBrush}"/>
            </MultiTrigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="ToggleButtonArrow" Property="Opacity" Value="0.5"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Key="MetroItemTemplate" BasedOn="{StaticResource {x:Type ComboBoxItem}}"
           TargetType="{x:Type ComboBoxItem}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ComboBoxItem">
                    <Border Name="Border" SnapsToDevicePixels="true" FocusVisualStyle="{x:Null}" BorderThickness="0" Padding="0" Visibility="{Binding Visibility}">
                        <Grid Name="Panel" Width="60" FocusVisualStyle="{x:Null}" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                 <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="60"/>
                            </Grid.ColumnDefinitions>
                            <Image Source="{Binding ExportImage}" Width="Auto" Height="16" HorizontalAlignment="Left" Margin="2,0,0,0" VerticalAlignment="Center"/>
                            <TextBlock Grid.Column="1" Padding="8,3,3,3" FocusVisualStyle="{x:Null}" Text="{Binding Export}" TextAlignment="Left" FontSize="14"/>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsHighlighted" Value="true">
                            <Setter TargetName="Border" Property="BorderBrush" Value="Transparent"/>
                            <Setter TargetName="Panel" Property="Background" Value="{StaticResource ActiveBackgroundBrush}"/>
                            <Setter Property="Foreground" Value="{StaticResource ActiveForegroundBrush}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter TargetName="Border" Property="BorderBrush" Value="Transparent"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="True">
                            <Setter TargetName="Border" Property="BorderBrush" Value="Transparent"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource HoverBackgroundBrush}"/>
                            <Setter TargetName="Border" Property="BorderBrush" Value="{StaticResource HoverBorderBrush}"/>
                            <Setter Property="Foreground" Value="{StaticResource HoverForegroundBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="MetroExportComboBox" TargetType="{x:Type ComboBox}" BasedOn="{StaticResource {x:Type ComboBox}}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ComboBox">
                    <Grid FocusVisualStyle="{x:Null}">
                        <ToggleButton x:Name="MetroTogg" Background="Transparent" Focusable="False" IsChecked="{Binding Path=IsDropDownOpen,Mode=TwoWay,RelativeSource={RelativeSource TemplatedParent}}" Template="{StaticResource MetroToggleButtonStyle}"/>
                        <Path Name="MetroPath" Data="M12.720993,5.1740319 L15.998993,8.3070474 12.720993,11.440032 12.720993,9.2730036 7.9499941,9.2730036 7.9499941,7.3410103 12.720993,7.3410103 z M0,0 L11.077997,0 11.077997,5.8950018 9.4339972,5.8950018 9.4339972,1.6120007 1.6429996,1.6120007 1.6429996,13.160004 9.4339972,13.160004 9.4339972,10.646003 11.077997,10.646003 11.077997,14.771004 0,14.771004 z" Fill="{StaticResource GlyphBrush}" Margin="4,0,0,0" HorizontalAlignment="Left" Height="16" Stretch="Fill" VerticalAlignment="Center" Width="16">                           
                            </Path>
                        <Popup Name="DropDownPopUp" FocusVisualStyle="{x:Null}" Placement="Bottom" IsOpen="{TemplateBinding IsDropDownOpen}" Focusable="False" PopupAnimation="Slide">
                            <Grid SnapsToDevicePixels="True" FocusVisualStyle="{x:Null}" MinWidth="{TemplateBinding ActualWidth}" MaxHeight="{TemplateBinding MaxDropDownHeight}">
                                <Border BorderBrush="White" Background="White" FocusVisualStyle="{x:Null}" BorderThickness="1,1,1,1"/>
                                <ScrollViewer BorderBrush="Transparent" FocusVisualStyle="{x:Null}" Padding="1" BorderThickness="0">
                                    <ItemsPresenter x:Name="ItemsPresenter" FocusVisualStyle="{x:Null}" KeyboardNavigation.DirectionalNavigation="Contained" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" Width="60"/>
                                </ScrollViewer>
                            </Grid>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="MetroPath" Property="Path.Fill" Value="{StaticResource GlyphBrush}"></Setter>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="MetroPath" Property="Opacity" Value="0.5"/>
                        </Trigger>
                        <DataTrigger Binding="{Binding IsPressed, ElementName=MetroTogg}" Value="True">
                            <Setter TargetName="MetroPath" Property="Fill" Value="{StaticResource ActiveGlyphBrush}"/>
                        </DataTrigger>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsPressed, ElementName=MetroTogg}" Value="True"/>
                                <Condition Binding="{Binding IsChecked, ElementName=MetroTogg}" Value="True"/>
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="MetroPath" Property="Fill" Value="{StaticResource ActiveGlyphBrush}"/>
                        </MultiDataTrigger>
                        <DataTrigger Binding="{Binding IsChecked, ElementName=MetroTogg}" Value="True">
                            <Setter TargetName="MetroPath" Property="Fill" Value="{StaticResource ActiveGlyphBrush}"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding IsMouseOver, ElementName=MetroTogg}" Value="True">
                            <Setter TargetName="MetroPath" Property="Fill" Value="{StaticResource HoverGlyphBrush}"/>
                        </DataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>                
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="MetroZoomComboBox" TargetType="{x:Type ComboBox}" BasedOn="{StaticResource {x:Type ComboBox}}">
        <Setter Property="Background" Value="Transparent"></Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ComboBox">
                    <Grid>
                        <ToggleButton Background="{StaticResource BackgroundBrush}" Foreground="{StaticResource ForegroundBrush}"  Focusable="False" IsChecked="{Binding Path=IsDropDownOpen,Mode=TwoWay,RelativeSource={RelativeSource TemplatedParent}}" Template="{StaticResource MetroZoomToggleButtonStyle}"/>
                        <Border BorderBrush="{StaticResource BorderBrush}" BorderThickness="1"/>
                        <ContentPresenter Name="ContentSite" IsHitTestVisible="False" Content="{TemplateBinding SelectionBoxItem}" ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}" ContentTemplateSelector="{TemplateBinding ItemTemplateSelector}" Margin="3,3,23,3" VerticalAlignment="Center" HorizontalAlignment="Left" />
                        <Popup Name="DropDownPopUp" Placement="Bottom" IsOpen="{TemplateBinding IsDropDownOpen}" Focusable="False" PopupAnimation="Slide">
                            <Grid SnapsToDevicePixels="True" MinWidth="{TemplateBinding ActualWidth}" MaxHeight="{TemplateBinding MaxDropDownHeight}" Background="{StaticResource BackgroundBrush}">
                                <Border BorderBrush="{StaticResource ContentBorderBrush}" BorderThickness="1,0,1,1"/>
                                <ScrollViewer BorderBrush="{StaticResource ContentBorderBrush}" Padding="1" BorderThickness="0">
                                    <ItemsPresenter x:Name="ItemsPresenter" KeyboardNavigation.DirectionalNavigation="Contained" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" Width="68"/>
                                </ScrollViewer>
                            </Grid>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

