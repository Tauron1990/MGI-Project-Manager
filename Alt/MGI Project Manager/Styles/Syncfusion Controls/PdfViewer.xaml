<ResourceDictionary  
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:pdfviewer="clr-namespace:Syncfusion.Windows.PdfViewer;assembly=Syncfusion.PdfViewer.WPF"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d">

 <ResourceDictionary.MergedDictionaries>

</ResourceDictionary.MergedDictionaries>

<LinearGradientBrush x:Key="BackgroundBrush" StartPoint="0,0" EndPoint="1,1" ><GradientStop Color="#FFEEEEF2" Offset="0" /><GradientStop Color="#FFEEEEF2" Offset="1" /></LinearGradientBrush>
    <SolidColorBrush x:Key="BorderBrush" Color="#FFCCCEDB" />
	<SolidColorBrush x:Key="ForegroundBrush" Color="#FF404040" />
	<SolidColorBrush x:Key="GlyphBrush" Color="#FF404040" />
<LinearGradientBrush x:Key="HeaderBackgroundBrush" StartPoint="0,0" EndPoint="1,1" ><GradientStop Color="#FFEEEEF2" Offset="0" /><GradientStop Color="#FFEEEEF2" Offset="1" /></LinearGradientBrush>
    <SolidColorBrush x:Key="HeaderBorderBrush" Color="#FFCCCEDB" />
	<SolidColorBrush x:Key="HeaderForegroundBrush" Color="#FF404040" />
	<SolidColorBrush x:Key="HeaderGlyphBrush" Color="#FF404040" />
<LinearGradientBrush x:Key="HoverBackgroundBrush" StartPoint="0,0" EndPoint="1,1" ><GradientStop Color="#FF7FBCE5" Offset="0" /><GradientStop Color="#FF7FBCE5" Offset="1" /></LinearGradientBrush>
    <SolidColorBrush x:Key="HoverBorderBrush" Color="#FF7FBCE5" />
	<SolidColorBrush x:Key="HoverForegroundBrush" Color="#FF404040" />
	<SolidColorBrush x:Key="HoverGlyphBrush" Color="#FF404040" />
<LinearGradientBrush x:Key="ActiveBackgroundBrush" StartPoint="0,0" EndPoint="1,1" ><GradientStop Color="#FF007ACC" Offset="0" /><GradientStop Color="#FF007ACC" Offset="1" /></LinearGradientBrush>
    <SolidColorBrush x:Key="ActiveBorderBrush" Color="#FF007ACC" />
	<SolidColorBrush x:Key="ActiveForegroundBrush" Color="#FFFFFFFF" />
	<SolidColorBrush x:Key="ActiveGlyphBrush" Color="#FFFFFFFF" />
	<SolidColorBrush x:Key="ContentBackgroundBrush" Color="#FFF5F5F5" />
	<SolidColorBrush x:Key="ContentBorderBrush" Color="#FFCCCEDB" />
	<SolidColorBrush x:Key="ContentForegroundBrush" Color="#FF1E1E1E" />
	<SolidColorBrush x:Key="ContentSelectionBrush" Color="#FFA5A5A5" />
	<SolidColorBrush x:Key="SelectionInActiveBrush" Color="#FF99C9EA" />
	<SolidColorBrush x:Key="ErrorBrush" Color="#FFFF0000" />
	<SolidColorBrush x:Key="ActiveLightBackground" Color="#FF99C9EA" />
	<SolidColorBrush x:Key="ActiveDarkBackground" Color="#FF003D66" />
	<SolidColorBrush x:Key="ContentAlternativeBackground" Color="#FFDCDCDC" />
	<SolidColorBrush x:Key="TickColor" Color="#FFBCBCBC" />
	<SolidColorBrush x:Key="RangeColor1" Color="#FF61A301" />	
	<SolidColorBrush x:Key="RangeColor2" Color="#FFF2E606" />	
	<SolidColorBrush x:Key="RangeColor3" Color="#FFFF0000" />
	<SolidColorBrush x:Key="LightBackgroundBrush" Color="#FFDDDDE5" />


    <Style x:Key="ButtonStyle" TargetType="Button">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid>
                        <Border x:Name="Background" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1" Background="Transparent">
                            <Grid Background="{TemplateBinding Background}" Margin="1">
                                <Border x:Name="BackgroundAnimation" Background="Transparent" Opacity="0"/>
                            </Grid>
                        </Border>
                        <ContentPresenter x:Name="contentPresenter" ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="True">
                            <Setter Property="Foreground" Value="{StaticResource GlyphBrush}"></Setter>
                            <Setter TargetName="Background" Property="BorderBrush" Value="Transparent"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" Value="{StaticResource GlyphBrush}"></Setter>
                            <Setter Property="Opacity" Value="0.5"></Setter>
                            <Setter TargetName="Background" Property="BorderBrush" Value="Transparent"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{StaticResource HoverGlyphBrush}"/>
                            <Setter Property="Background" Value="{StaticResource HoverBackgroundBrush}"></Setter>
                            <Setter TargetName="Background" Property="BorderBrush" Value="{StaticResource HoverBorderBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" Value="{StaticResource ActiveBackgroundBrush}" />
                            <Setter Property="Foreground" Value="{StaticResource ActiveGlyphBrush}" />
                            <Setter TargetName="Background" Property="BorderBrush" Value="{StaticResource ActiveBorderBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

	<Style x:Key="ToggleButtonStyle" TargetType="ToggleButton">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToggleButton">
                    <Grid>
                        <Border x:Name="Background" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0" Background="Transparent"  CornerRadius="3">
                            <Grid Background="{TemplateBinding Background}" Margin="1">
                                <Border x:Name="BackgroundAnimation" Background="Transparent" Opacity="0"/>
                            </Grid>
                        </Border>
                        <ContentPresenter x:Name="contentPresenter" ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        <Rectangle x:Name="DisabledVisualElement" Fill="Transparent" IsHitTestVisible="false" Opacity="0" RadiusY="3" RadiusX="3"/>
                        <Rectangle x:Name="FocusVisualElement" IsHitTestVisible="false" Margin="1" Opacity="0" RadiusY="2" RadiusX="2" Stroke="#FF6DBDD1" StrokeThickness="1"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="True">
                            <Setter Property="Foreground" Value="{StaticResource GlyphBrush}"></Setter>
                            <Setter TargetName="Background" Property="BorderBrush" Value="Transparent"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" Value="{StaticResource GlyphBrush}"></Setter>
                            <Setter Property="Opacity" Value="0.5"></Setter>
                            <Setter TargetName="Background" Property="BorderBrush" Value="Transparent"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{StaticResource HoverGlyphBrush}"/>
                            <Setter Property="Background" Value="{StaticResource HoverBackgroundBrush}"></Setter>
                            <Setter TargetName="Background" Property="BorderBrush" Value="{StaticResource HoverBorderBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" Value="{StaticResource ActiveBackgroundBrush}" />
                            <Setter Property="Foreground" Value="{StaticResource ActiveGlyphBrush}" />
                            <Setter TargetName="Background" Property="BorderBrush" Value="{StaticResource ActiveBorderBrush}"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Background" Value="{StaticResource ActiveBackgroundBrush}" />
                            <Setter Property="Foreground" Value="{StaticResource ActiveGlyphBrush}" />
                            <Setter TargetName="Background" Property="BorderBrush" Value="{StaticResource ActiveBorderBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
	
    <Style x:Key="ViewerCloseButtonStyle" TargetType="Button">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border x:Name="Bd" SnapsToDevicePixels="true" Background="Transparent" BorderBrush="Transparent" BorderThickness="0"  Width="Auto" Height="Auto">
                        <ContentPresenter x:Name="contentPresenter"></ContentPresenter>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="True">
                            <Setter Property="Foreground" Value="{StaticResource GlyphBrush}"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{StaticResource HoverGlyphBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type pdfviewer:PdfViewerControl}">
        <Setter Property="TextSelectionBrushColor" Value="#009EFF"></Setter>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type pdfviewer:PdfViewerControl}" >
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <StackPanel x:Name="container" Width="{TemplateBinding Width}" Height="{TemplateBinding Height}">
                            <pdfviewer:DocumentToolbar x:Name="PART_Toolbar" Width="Auto" Height="Auto" />
                            <Grid Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" >
                                <pdfviewer:PdfDocumentView x:Name="PART_DocumentView" 
                                                   Width="Auto"
                                                   ScrollViewer.HorizontalScrollBarVisibility="Auto" 
                                                   ScrollViewer.VerticalScrollBarVisibility="Auto" />
                                <pdfviewer:NotificationBar x:Name="PART_NotificationBar" Width="Auto" Height="Auto" VerticalAlignment="Top" />
                                <pdfviewer:TextSearchBar x:Name="PART_TextSearchBar" Width="Auto" Height="Auto" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="5,5,25,25" ></pdfviewer:TextSearchBar>
                            </Grid>
                        </StackPanel>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="ProgressBarStyle">

    </Style>
    <Style TargetType="{x:Type pdfviewer:CopyProgressIndicator}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type pdfviewer:CopyProgressIndicator}" >
                    <Border Name="SearchProgressBarBorder" Background="{StaticResource ContentBackgroundBrush}" BorderBrush="{StaticResource ContentBorderBrush}" Opacity="0.7" BorderThickness="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9*" />
                                <ColumnDefinition Width="1*" />
                            </Grid.ColumnDefinitions>
                            <ProgressBar Name="PART_ProgressBar" Grid.Column="0" Height="3" Width="208" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{StaticResource ProgressBarStyle}"></ProgressBar>
                            <Button Name="PART_CloseButton" Style="{StaticResource ViewerCloseButtonStyle}" Grid.Column="1" VerticalAlignment="Center" BorderBrush="Transparent" HorizontalAlignment="Left" Height="12" Width="12" Background="Transparent" >
                            <Path Name="PART_ButtonPath" Data="M0,0 L1,1 M0,1 L1,0" StrokeThickness="2" Stretch="Fill" Stroke="{Binding Path=Foreground,RelativeSource={RelativeSource AncestorType={x:Type Button}}}">
                                </Path>
                            </Button>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type pdfviewer:DocumentView}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type pdfviewer:DocumentView}" >
                    <Grid Name="homegrid" Background="{StaticResource BackgroundBrush}">
                        <ItemsControl  Name="PART_ItemsControl" ItemsSource="{Binding}" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto">
                            <ItemsControl.Template>
                                <ControlTemplate TargetType="{x:Type ItemsControl}">
                                    <ScrollViewer Name="PART_ScrollViewer" CanContentScroll="True" FocusVisualStyle="{x:Null}">
                                        <ItemsPresenter />
                                    </ScrollViewer>
                                </ControlTemplate>
                            </ItemsControl.Template>
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <ItemsControl ItemsSource="{Binding}">
                                        <ItemsControl.Template>
                                            <ControlTemplate TargetType="{x:Type ItemsControl}">
                                                <ItemsPresenter />
                                            </ControlTemplate>
                                        </ItemsControl.Template>
                                    </ItemsControl>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <pdfviewer:CustomVPanel IsItemsHost="True"/>
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                        </ItemsControl>

                        <pdfviewer:PdfProgressIndicator Visibility="Hidden" PageBeingInitialized="{Binding PageBeingInitialized, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource 
                                   Mode=FindAncestor, AncestorType=UserControl}}" PageCount="{Binding PageCount, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource 
                                   Mode=FindAncestor, AncestorType=UserControl}}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="5,5,25,25" Width="127" Height="46" x:Name="PART_ProgressIndicator"></pdfviewer:PdfProgressIndicator>

                        <pdfviewer:PdfLoadingIndicator x:Name="PART_Loader"  Visibility="Hidden" ></pdfviewer:PdfLoadingIndicator>
                        <pdfviewer:CopyProgressIndicator x:Name="PART_CopyProgressIndicator" Visibility="Collapsed" VerticalAlignment="Bottom" HorizontalAlignment="Right" Height="46" Width="262" Margin="5,5,25,25"></pdfviewer:CopyProgressIndicator>
                        <pdfviewer:TextSearchProgressIndicator x:Name="PART_SearchProgresIndicator" Visibility="Collapsed" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="5,5,25,25"></pdfviewer:TextSearchProgressIndicator>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type pdfviewer:PdfLoadingIndicator}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type pdfviewer:PdfLoadingIndicator}" >
                    <Grid>
                        <Viewbox HorizontalAlignment="Center" Height="50" Margin="108,100,92,100" VerticalAlignment="Center" Width="50" RenderTransformOrigin="0.52,0.477">
                            <Canvas Width="45" Height="45"
                HorizontalAlignment="Left"
                VerticalAlignment="Top">
                                <TextBlock Name="PART_Loadertext" HorizontalAlignment="Center" Margin="0,-50" Text="{Binding LoadingMessage, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource 
                                   Mode=FindAncestor, AncestorType=UserControl}}" FontSize="12" Foreground="#FF475F77"></TextBlock>

                                <Path Data="M50,27.5 C50,24.23333 45,24.23333 45,27.5 C45,30.83333 50,30.83333 50,27.5" Name="PART_PathA"
                  Fill="{StaticResource HoverBorderBrush}"
                  RenderTransformOrigin="0.5,0.83333">
                                    <Path.RenderTransform >
                                        <RotateTransform x:Name="RotateA" Angle="0"/>
                                    </Path.RenderTransform>
                                    <Path.Triggers>
                                        <EventTrigger RoutedEvent="Path.Loaded">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <DoubleAnimationUsingKeyFrames Storyboard.TargetName="RotateA"
                                                               Storyboard.TargetProperty="Angle"
                                                               RepeatBehavior="Forever">
                                                        <LinearDoubleKeyFrame KeyTime="0:0:0" Value="360"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.0" Value="0">

                                                        </LinearDoubleKeyFrame>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.5" Value="0"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.55" Value="0"/>
                                                    </DoubleAnimationUsingKeyFrames>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Path.Triggers>
                                </Path>
                                <Path Data="M50,27.5 C50,24.23333 45,24.23333 45,27.5 C45,30.83333 50,30.83333 50,27.5" Name="PART_PathB"
                   Fill="{StaticResource HoverBorderBrush}"
                  RenderTransformOrigin="0.5,0.83333">
                                    <Path.RenderTransform>
                                        <RotateTransform x:Name="RotateB" Angle="13"/>
                                    </Path.RenderTransform>
                                    <Path.Triggers>
                                        <EventTrigger RoutedEvent="Path.Loaded">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <DoubleAnimationUsingKeyFrames Storyboard.TargetName="RotateB"
                                                               Storyboard.TargetProperty="Angle"
                                                               RepeatBehavior="Forever">
                                                        <LinearDoubleKeyFrame KeyTime="0:0:0" Value="0"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:0.1" Value="0"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.1" Value="-347">

                                                        </LinearDoubleKeyFrame>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.5" Value="-360"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.55" Value="-360"/>
                                                    </DoubleAnimationUsingKeyFrames>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Path.Triggers>
                                </Path>
                                <Path Data="M50,27.5 C50,24.23333 45,24.23333 45,27.5 C45,30.83333 50,30.83333 50,27.5" Name="PART_PathC"
                  Fill="{StaticResource HoverBorderBrush}"
                  RenderTransformOrigin="0.5,0.83333">
                                    <Path.RenderTransform>
                                        <RotateTransform x:Name="RotateC" Angle="26"/>
                                    </Path.RenderTransform>
                                    <Path.Triggers>
                                        <EventTrigger RoutedEvent="Path.Loaded">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <DoubleAnimationUsingKeyFrames Storyboard.TargetName="RotateC"
                                                               Storyboard.TargetProperty="Angle"
                                                               RepeatBehavior="Forever">
                                                        <LinearDoubleKeyFrame KeyTime="0:0:0" Value="0"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:0.2" Value="0"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.2" Value="-334">

                                                        </LinearDoubleKeyFrame>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.5" Value="-360"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.55" Value="-360"/>
                                                    </DoubleAnimationUsingKeyFrames>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Path.Triggers>
                                </Path>

                                <Path Data="M50,27.5 C50,24.23333 45,24.23333 45,27.5 C45,30.83333 50,30.83333 50,27.5" Name="PART_PathD"
                   Fill="{StaticResource HoverBorderBrush}"
                  RenderTransformOrigin="0.5,0.83333">
                                    <Path.RenderTransform>
                                        <RotateTransform x:Name="RotateD" Angle="39"/>
                                    </Path.RenderTransform>
                                    <Path.Triggers>
                                        <EventTrigger RoutedEvent="Path.Loaded">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <DoubleAnimationUsingKeyFrames Storyboard.TargetName="RotateD"
                                                               Storyboard.TargetProperty="Angle"
                                                               RepeatBehavior="Forever">
                                                        <LinearDoubleKeyFrame KeyTime="0:0:0" Value="0"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:0.3" Value="0"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.3" Value="-321">

                                                        </LinearDoubleKeyFrame>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.5" Value="-360"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.55" Value="-360"/>
                                                    </DoubleAnimationUsingKeyFrames>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Path.Triggers>
                                </Path>
                                <Path Data="M50,27.5 C50,24.23333 45,24.23333 45,27.5 C45,30.83333 50,30.83333 50,27.5" Name="PART_PathE"
                   Fill="{StaticResource HoverBorderBrush}"
                  RenderTransformOrigin="0.5,0.83333">
                                    <Path.RenderTransform>
                                        <RotateTransform x:Name="RotateE" Angle="52"/>
                                    </Path.RenderTransform>
                                    <Path.Triggers>
                                        <EventTrigger RoutedEvent="Path.Loaded">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <DoubleAnimationUsingKeyFrames Storyboard.TargetName="RotateE"
                                                               Storyboard.TargetProperty="Angle"
                                                               RepeatBehavior="Forever">
                                                        <LinearDoubleKeyFrame KeyTime="0:0:0" Value="0"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:0.4" Value="0"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.4" Value="-308">

                                                        </LinearDoubleKeyFrame>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.5" Value="-360"/>
                                                        <LinearDoubleKeyFrame KeyTime="0:0:1.55" Value="-360"/>
                                                    </DoubleAnimationUsingKeyFrames>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Path.Triggers>
                                </Path>
                            </Canvas>
                        </Viewbox>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type pdfviewer:PdfProgressIndicator}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type pdfviewer:PdfProgressIndicator}" >
                    <Border BorderBrush="{StaticResource ContentBorderBrush}" Background="{StaticResource ContentBackgroundBrush}" Opacity="0.6" BorderThickness="2" CornerRadius="4,4,4,4">
                        <Grid  Margin="12,2,0,0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="15"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <TextBlock Name="PART_LoadingPage" Grid.Row="0" Margin="10,0,0,0" Text="Loading page" FontSize="10" Foreground="{StaticResource ContentForegroundBrush}"></TextBlock>
                            <StackPanel Grid.Row="1" Orientation="Horizontal">
                                <TextBlock Name="PART_PageBeingInitializedLabel" Margin="10,0,0,0" FontWeight="Bold" Text="{Binding PageBeingInitialized}" FontSize="14" Foreground="{StaticResource ContentForegroundBrush}"></TextBlock>
                                <TextBlock Name="PART_of" Margin="2,0" Text=" of " FontSize="14" Foreground="{StaticResource ContentForegroundBrush}"></TextBlock>
                                <TextBlock Name="PART_PageCountLabel" FontWeight="Bold" Text="{Binding PageCount}" FontSize="14" Foreground="{StaticResource ContentForegroundBrush}"></TextBlock>
                            </StackPanel>
                        </Grid>
                    </Border>

                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type pdfviewer:NotificationBar}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type pdfviewer:NotificationBar}" >

                    <Border Width="Auto" BorderThickness="1" BorderBrush="#B4CBE8">
                        <Border.Background >
                            <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                <GradientStop Color="#B4CBE8" Offset=".5"/>
                                <GradientStop Color="#CFDFF4" Offset="0"/>
                            </LinearGradientBrush>
                        </Border.Background>

                        <Grid>
                            <DockPanel>
                                <Path Visibility="Collapsed" Name="PathA" Data="F1 M 44.55,43.58 L 54.01,53.03 52.89,54.15 43.43,44.7 44.55,43.58 z"   />
                                <Path Visibility="Collapsed" Name="PathB" Data="F1 M 52.89,43.42 L 54.01,44.54 44.55,54.15 43.43,53.03 52.89,43.42 z" />
                                <Button DockPanel.Dock="Right" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="6,7,5,5" Name="PART_button1" Width="15" Height="Auto" Background="Transparent" BorderBrush="Transparent">

                                    <Path Name="ClosePath" Fill="Black"  Stretch="Fill" StrokeThickness="2">
                                        <Path.Data>
                                            <CombinedGeometry GeometryCombineMode="Union"
                                                          Geometry1="{Binding ElementName=PathA, Path=Data}"
                                  Geometry2="{Binding ElementName=PathB, Path=Data}" />
                                        </Path.Data>
                                    </Path>
                                </Button>
                                <Label DockPanel.Dock="Left" HorizontalAlignment="Left" VerticalAlignment="Center" Name="PART_label1" Width="Auto" >Label</Label>
                                <TextBlock Name="PART_LinkLabel" DockPanel.Dock="Right" HorizontalAlignment="Right" VerticalAlignment="Center">
                <Hyperlink>
                    Copy exception details to the clipboard
                </Hyperlink>
                                </TextBlock>
                            </DockPanel>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type pdfviewer:PageNumberToolTip}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type pdfviewer:PageNumberToolTip}" >
                    <Grid  Opacity="0.7">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="80*" />
                            <ColumnDefinition Width="20*" />
                        </Grid.ColumnDefinitions>
                        <Border BorderBrush="{StaticResource ContentBorderBrush}" Background="{StaticResource ContentBackgroundBrush}"  CornerRadius="7" Height="50" Width="70" Grid.Column="0">
                            <ContentControl VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="{StaticResource ContentForegroundBrush}" Name="PART_PageDisplay"/>
                        </Border>
                        <Border BorderBrush="{StaticResource ContentBorderBrush}" Grid.Column="1" Background="{StaticResource ContentBackgroundBrush}" Margin="-10,0,0,0">
                            <ContentControl VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="{StaticResource ContentBorderBrush}" Background="{StaticResource ContentBorderBrush}">
                                <Path Data="F1M1188.2,1715.23L1169.93,1728.57 1151.66,1741.9 1151.66,1715.23 1151.66,1688.57 1169.93,1701.9 1188.2,1715.23z" Stretch="Uniform" Fill="Gray" Width="26" Height="26"/>
                            </ContentControl>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="TextBoxStyle" TargetType="TextBox">
        <Setter Property="Foreground" Value="{StaticResource ContentForegroundBrush}"/>
		<Setter Property="SelectionBrush" Value="{StaticResource ContentForegroundBrush}"></Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Border x:Name="Bd" SnapsToDevicePixels="true" Background="White" BorderThickness="1"  Width="Auto" Height="Auto">
                        <ScrollViewer x:Name="PART_ContentHost"/>
                    </Border>
                    <ControlTemplate.Triggers>
					<Trigger Property="IsEnabled" Value="True">
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource ContentBackgroundBrush}"></Setter>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="False">
                            <Setter Property="BorderBrush"  Value="{StaticResource BorderBrush}" TargetName="Bd"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="BorderBrush"  Value="{StaticResource HoverBorderBrush}" TargetName="Bd"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="TextSearchButtonStyle" TargetType="Button">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid>
                        <Border x:Name="Background" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1" Background="Transparent">
                            <Grid Background="{TemplateBinding Background}" Margin="1">
                                <Border x:Name="BackgroundAnimation" Background="Transparent" Opacity="0"/>
                            </Grid>
                        </Border>
                        <ContentPresenter x:Name="contentPresenter" ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="True">
                            <Setter Property="Foreground" Value="{StaticResource GlyphBrush}"></Setter>
                            <Setter Property="Background" Value="{StaticResource BackgroundBrush}"/>
                            <Setter Property="BorderBrush" Value="{StaticResource BorderBrush}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" Value="{StaticResource GlyphBrush}"></Setter>
                            <Setter Property="Background" Value="{StaticResource BackgroundBrush}"/>
                            <Setter Property="BorderBrush" Value="{StaticResource BorderBrush}"/>
                            <Setter Property="Opacity" Value="0.5"></Setter>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{StaticResource HoverGlyphBrush}"/>
                            <Setter Property="Background" Value="{StaticResource HoverBackgroundBrush}"></Setter>
                            <Setter Property="BorderBrush" Value="{StaticResource HoverBorderBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" Value="{StaticResource ActiveBackgroundBrush}" />
                            <Setter Property="Foreground" Value="{StaticResource ActiveGlyphBrush}" />
                            <Setter Property="BorderBrush" Value="{StaticResource ActiveBorderBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    
	
