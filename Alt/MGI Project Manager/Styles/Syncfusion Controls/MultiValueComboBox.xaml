    <ControlTemplate x:Key="ToggleButtonStyle_Metro" TargetType="ToggleButton">

        <Grid FocusVisualStyle="{x:Null}">
            <Grid.RowDefinitions>
                <RowDefinition Height="25" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition Width="20" />
            </Grid.ColumnDefinitions>
            <Border
                x:Name="ButtonBorder"
                Grid.ColumnSpan="2"
                Background="{TemplateBinding Background}"
                BorderBrush="#bfbfbf"
                BorderThickness="1"
                FocusVisualStyle="{x:Null}"
                SnapsToDevicePixels="True" />
            <TextBlock
                Name="Text"
                Height="24"
                Padding="5,2,0,0"
                VerticalAlignment="Center"
                FocusVisualStyle="{x:Null}"
                FontSize="14"
                Text="{TemplateBinding Content}"
                ToolTip="{TemplateBinding Content}">
                <TextBlock.Style>
                    <Style TargetType="TextBlock">
                        <Style.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Background" Value="LightGray" />
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </TextBlock.Style>
            </TextBlock>
            <Border
                x:Name="splitBorder"
                Grid.Column="1"
                Width="20"
                Margin="0"
                HorizontalAlignment="Right"
                BorderBrush="#bfbfbf"
                BorderThickness="1,0,0,0"
                FocusVisualStyle="{x:Null}"
                SnapsToDevicePixels="true">
                <Border.Style>
                    <Style TargetType="Border">
                        <Style.Triggers>
                            <Trigger Property="ToggleButton.IsMouseOver" Value="True">
                                <Setter Property="Background" Value="LightGray" />
                                <Setter Property="Background" Value="LightGray" />
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Border.Style>
                <Path
                    x:Name="ToggleButtonArrow"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Data="{StaticResource DownArrowGeometry}"
                    Fill="#FF333333" />
            </Border>
        </Grid>

        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="ToggleButtonArrow" Property="Fill" Value="#AFAFAF" />
            </Trigger>
            <Trigger Property="ToggleButton.IsChecked" Value="True">
                <Setter TargetName="splitBorder" Property="Background" Value="#00a0d8" />
                <Setter TargetName="splitBorder" Property="BorderBrush" Value="#00a0d8" />
                <Setter TargetName="ToggleButtonArrow" Property="Fill" Value="Black " />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

