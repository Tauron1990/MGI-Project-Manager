<Window x:Class="IISServiceManager.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IISServiceManager"
        xmlns:terminal="clr-namespace:Simple.Wpf.Terminal;assembly=Simple.Wpf.Terminal"
        mc:Ignorable="d" WindowStartupLocation="CenterOwner"
        Title="Log Window" Height="450" Width="800">
    <terminal:Terminal IsReadOnly="True" ItemsSource="{Binding Lines}">

    </terminal:Terminal>
</Window>
